

<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title - My ASP.NET MVC Application</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
        
        <script src="~/Scripts/jquery-2.1.1.js"></script>
        <script src="~/Scripts/jquery-ui-1.11.1.js"></script>
        <script type='text/javascript' src='/Scripts/doctorviewmodel.js'></script>     
    

    </head>
    <body>
        <header>
            <div id="menu" class="navbar navbar-default navbar-fixed-top">
                <div class="navbar-header">
                    <button class="btn btn-success navbar-toggle">
                        Menu
                    </button>
                    <div class= "float-right">
                        <p class="site-title"><h1 style ="color: White">Health Magnagement System</h1></p>
                       
                        </div>
                    </div>
                        <div class="navbar-collapse collapse">
                        
                            <ul class="nav navbar-nav navbar-right">
                                <li class =" nav">@Html.ActionLink("Home", "Index", "Doctor")</li>
                                <li class=" nav">@Html.ActionLink("Create Doctor", "Create", "Doctor")</li>
                                
                            </ul>
                        
                    </div>
            </div>
        </header>
        <div id="body" style="margin-top: 110px">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <footer class =" container">
            <div class="content-wrapper">
                <div class="float-left">
                   
                </div>
            </div>
        </footer>

        @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)
    </body>
</html>
