<ul class="pager">
    <li class="previous"><a href="/Doctor/index/">&larr; Prev</a></li>

</ul>

    <h2 class="col-lg-10 col-md-8 col-sm-6 col-xs-4 col-md-offset-1">@ViewBag.Title</h2>

    <p style="color: red" data-bind="text: MessageToClient" class="col-lg-10 col-md-8 col-sm-6 col-xs-4 col-md-offset-1"></p>

    <div>
        <div class="form-group col-lg-2 col-md-2  col-md-offset-1">
            <label class="control-label">Doctor Name: </label>
            <span class="form-control" data-bind="text: DoctorName"></span>
        </div>
        <div class="form-group col-lg-2 col-md-2  col-md-offset-1">
            <label class="control-label">Practice Name: </label>
            <span class="form-control" data-bind="text: PracticeName"></span>
        </div>
    </div>
<div class="col-lg-10 col-md-8 col-sm-6 col-xs-4 col-md-offset-1">
    <table class="table table-striped table-bordered table-hover table-condensed">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Street Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
            <th>Phone Number</th>

        </tr>
        <tbody data-bind="foreach: Patients">
            <tr>
                <td class="form-group"><span class="form-control input-sm" data-bind="text: FirstName"></span></td>
                <td class="form-group"><span class="form-control input-sm" data-bind="text: LastName"></span></td>
                <td class="form-group"><span class="form-control input-sm" data-bind="text: Street"></span></td>
                <td class="form-group"><span class="form-control input-sm" data-bind="text: City"></span></td>
                <td class="form-group"><span class="form-control input-sm" data-bind="text: State"></span></td>
                <td class="form-group"><span class="form-control input-sm" data-bind="text: ZipCode"></span></td>
                <td class="form-group"><span class="form-control input-sm" data-bind="text: PhoneNumber"></span></td>

            </tr>
        </tbody>
    </table>

</div>

