<h2>@ViewBag.Title</h2>

<p data-bind="text: MessageToClient"></p>

<div>
    <div class ="form-group">
        <label class ="control-label" for ="DoctorName">Doctor Name: </label>
        <input class ="form-control"name = "DoctorName" id = "DoctorName" data-bind="value: DoctorName, event: {change: flagSalesOrderAsEdited}" />
    </div>
    <div class ="form-group">
        <label class ="control-label" for ="PracticeName">Practice Name: </label>
        <input class =" form-control"name = "PracticeName" id = "PracticeName" data-bind="value: PracticeName, event: {change: flagSalesOrderAsEdited}" />
    </div>
</div>

<p><button data-bind="click: save" class="btn btn-primary">Save</button></p>

<p>

    <a href="/" class =" btn btn-default btn-sm">&laquo;Back to List</a>
</p>
